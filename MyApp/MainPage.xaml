<Page
    x:Class="MyApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MyApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Canvas HorizontalAlignment="Center" Height="480" Margin="0,0,0,0" VerticalAlignment="Center" Width="800">
            <Image x:Name="image" Height="480" Source="Assets/particulairsplash.png" Width="800" Stretch="Fill"/>
            <TextBlock x:Name="txtH" Text="0.00 %"  Height="38" FontSize="20" Canvas.Top="326" Width="178" Canvas.Left="114"  HorizontalAlignment="Left" FontWeight="Bold" Foreground="White" />
            <TextBlock x:Name="txtT" Text="0.00 *F"  Height="38" FontSize="20" Width="145" Canvas.Top="264" Canvas.Left="147"  HorizontalAlignment="Left" FontWeight="Bold" Foreground="White"  />
            
        </Canvas>


    </Grid>
</Page>
